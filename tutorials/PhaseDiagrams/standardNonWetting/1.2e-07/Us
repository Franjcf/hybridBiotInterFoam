FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1.2e-07";
    object      Us;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            slip;
    }
    upperInlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    lowerInlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    rightInlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    leftInlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    walls2
    {
        type            slip;
    }
}


