FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.2e-07";
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform 6.7e-11;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            zeroGradient;
    }
    upperInlet
    {
        type            zeroGradient;
    }
    lowerInlet
    {
        type            zeroGradient;
    }
    rightInlet
    {
        type            zeroGradient;
    }
    leftInlet
    {
        type            zeroGradient;
    }
    walls2
    {
        type            zeroGradient;
    }
}


