FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

//5e-14 = 50mD, 0.322^3/0.678^2 = 0.0726 
internalField   uniform 1.1e-11; //this times above should give 1e-10

boundaryField
{
    leftFace
    {
        type            zeroGradient;
    }

    walls
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

